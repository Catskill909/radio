{
    "typescript.tsserver.maxTsServerMemory": 4096,
    "typescript.preferences.autoImportFileExcludePatterns": [
        "node_modules/@prisma/client"
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.next/**": true,
        "**/dist/**": true
    },
    "files.exclude": {
        "**/*.db": true,
        "**/*.db-journal": true,
        "**/*.db-shm": true,
        "**/*.db-wal": true,
        "**/*.SAFE_BACKUP_*": true,
        "**/*.backup_*": true
    },
    "search.exclude": {
        "**/*.db": true,
        "**/*.db-journal": true
    }
}